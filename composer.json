{
    "name": "mobilexco/laravel-tokenizer",
    "description": "Laravel model attribute and/or string token replacements",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrique Ouimet",
            "email": "patrique.ouimet@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mobilexco\\Tokenizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mobilexco\\Tokenizer\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "orchestra/testbench": "^3.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mobilexco\\Tokenizer\\TokenizerServiceProvider"
            ]
        }
    }
}
